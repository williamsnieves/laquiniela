	position relative

.quiniela-header section .menu-quiniela ul
	list-style none
	margin 0

.quiniela-header section .menu-quiniela li
	position relative
	float left
	background azul_claro
	border-right 1px solid white
	-webkit-transform skewx(-20deg)
	transform skewx(-20deg)
	-webkit-transition background .2s ease, color .2s ease
	transition background .2s ease, color .2s ease
	&:hover
		background verde_claro

.quiniela-header section .menu-quiniela ul li a
	display block
	padding 1em
	color white
	&:hover
		color azul_claro

